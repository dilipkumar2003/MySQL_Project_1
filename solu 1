/* to create database*/
create database dilipkumar;
/* to create a table*/
create table dilipkumar.student(
s_no int,
name varchar(20),
roll_no varchar(20),
dept varchar(20),
clg_name varchar(20),
place varchar(20)
);
select * from dilipkumar.student;
describe dilipkumar.student;

/*to insert the values in table*/ 
